{
  "name": "vscode-line-number",
  "displayName": "Line Number",
  "description": "Insert and Delete line numbers in text and copy.",
  "version": "2.2.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-line-number"
  },
  "engines": {
    "vscode": "^1.62.3"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "line number"
  ],
  "activationEvents": [
    "onCommand:LineNumber.EditInsertFileLineNumberNoFormat",
    "onCommand:LineNumber.EditInsertFileLineNumberDeleteIndent",
    "onCommand:LineNumber.EditInsertInputStartNoFormat",
    "onCommand:LineNumber.EditInsertInputStartDeleteIndent",
    "onCommand:LineNumber.EditDeleteLineNumber",
    "onCommand:LineNumber.EditLineNumberTextDeleteBlankLine",
    "onCommand:LineNumber.EditLineNumberTextDeleteIndent",
    "onCommand:LineNumber.CopyNoHeaderNoFormat",
    "onCommand:LineNumber.CopyNoHeaderDeleteIndent",
    "onCommand:LineNumber.CopyFileNameNoFormat",
    "onCommand:LineNumber.CopyFileNameDeleteIndent",
    "onCommand:LineNumber.CopyFullPathNoFormat",
    "onCommand:LineNumber.CopyFullPathDeleteIndent",
    "onCommand:LineNumber.CopyRelativePathNoFormat",
    "onCommand:LineNumber.CopyRelativePathDeleteIndent",
    "onCommand:LineNumber.CopyDeleteLineNumber",

    "onCommand:LineNumber.SelectFunction"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "LineNumber.EditInsertFileLineNumberNoFormat",
        "title": "Line Number | Edit | Insert File LineNumber | No Format"
      },
      {
        "command": "LineNumber.EditInsertFileLineNumberDeleteIndent",
        "title": "Line Number | Edit | Insert File LineNumber | Delete Indent"
      },
      {
        "command": "LineNumber.EditInsertInputStartNoFormat",
        "title": "Line Number | Edit | Insert Input Start | No Format"
      },
      {
        "command": "LineNumber.EditInsertInputStartDeleteIndent",
        "title": "Line Number | Edit | Insert Input Start | Delete Indent"
      },
      {
        "command": "LineNumber.EditDeleteLineNumber",
        "title": "Line Number | Edit | Delete LineNumber"
      },
      {
        "command": "LineNumber.EditLineNumberTextDeleteBlankLine",
        "title": "Line Number | Edit | Edit LineNumber Text | Delete Blank Line"
      },
      {
        "command": "LineNumber.EditLineNumberTextDeleteIndent",
        "title": "Line Number | Edit | Edit LineNumber Text | Delete Indent"
      },
      {
        "command": "LineNumber.CopyNoHeaderNoFormat",
        "title": "Line Number | Copy With LineNumber | No Header | No Format"
      },
      {
        "command": "LineNumber.CopyNoHeaderDeleteIndent",
        "title": "Line Number | Copy With LineNumber | No Header | Delete Indent"
      },
      {
        "command": "LineNumber.CopyFileNameNoFormat",
        "title": "Line Number | Copy With LineNumber | Header Filename | No Format"
      },
      {
        "command": "LineNumber.CopyFileNameDeleteIndent",
        "title": "Line Number | Copy With LineNumber | Header Filename | Delete Indent"
      },
      {
        "command": "LineNumber.CopyFullPathNoFormat",
        "title": "Line Number | Copy With LineNumber | Header FullPath | No Format"
      },
      {
        "command": "LineNumber.CopyFullPathDeleteIndent",
        "title": "Line Number | Copy With LineNumber | Header FullPath | Delete Indent"
      },
      {
        "command": "LineNumber.CopyRelativePathNoFormat",
        "title": "Line Number | Copy With LineNumber | Header RelativePath | No Format"
      },
      {
        "command": "LineNumber.CopyRelativePathDeleteIndent",
        "title": "Line Number | Copy With LineNumber | Header RelativePath | Delete Indent"
      },
      {
        "command": "LineNumber.CopyDeleteLineNumber",
        "title": "Line Number | Copy Delete LineNumber"
      },
      {
        "command": "LineNumber.SelectFunction",
        "title": "Line Number | Select Function"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "LineNumber",
      "properties": {
        "LineNumber.subMenuChar": {
          "type": "string",
          "default": "â–¸"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "vsce package && code --install-extension vscode-line-number-2.2.0.vsix",
    "": ""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "vsce": "^2.5.1"
  }
}
